{
	"name": "net23",
	"description": "Network 23",
	"license": "GPL-3.0-only",
	"version": "0.1.0",
	"type": "module",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/zootella/cold3.git"
	},
	"scripts": {
		"local": "serverless offline --reloadHandler",
		"build": "serverless package && yarn size",
		"size": "node -e \"let fs=require('fs'); let b=fs.statSync('.serverless/net23.zip').size; console.log('serverless bundle net23.zip is '+b.toLocaleString()+' bytes')\"",
		"deploy-before": "yarn stowaway && yarn --cwd ../ seal set-cloud",
		"deploy-after": "yarn --cwd ../ seal set-local && yarn --cwd ../ test && yarn size",
		"deploy": "yarn deploy-before && serverless deploy && yarn deploy-after",
		"deploy-only": "serverless deploy --package .serverless",
		"stowaway": "node stowaway.js",
		"www": "aws s3 sync ./www s3://www-net23-cc --delete"
	},
	"dependencies": {
		"@sendgrid/mail": "^8.1.4",
		"@supabase/supabase-js": "^2.39.8",
		"icarus": "*",
		"sharp": "^0.34.4",
		"twilio": "^5.3.5"
	},
	"devDependencies": {
		"@aws-sdk/client-ses": "^3.682.0",
		"@aws-sdk/client-sns": "^3.682.0",
		"fs-extra": "^11.2.0",
		"serverless": "^4.4.11",
		"serverless-domain-manager": "^8.0.0",
		"serverless-offline": "^14.3.4"
	}
}
