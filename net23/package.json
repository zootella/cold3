{
  "name": "net23",
  "description": "Network 23",
  "license": "GPL-3.0-only",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zootella/cold3.git"
  },
  "scripts": {
    "local": "serverless offline --reloadHandler",
    "build": "serverless package && yarn size",
    "size": "ls -lh .serverless/net23.zip",
    "deploy": "serverless deploy && yarn size && yarn --cwd ../ run test",
    "justdeploy": "serverless deploy --package .serverless",
    "stowaway": "node stowaway.js",
    "www": "aws s3 sync ./www s3://www-net23-cc --delete"
  },
  "dependencies": {
    "icarus": "*",
    "@supabase/supabase-js": "2",
    "twilio": "5",
    "@sendgrid/mail": "8",
    "sharp": "0"
  },
  "devDependencies": {
    "fs-extra": "11",
    "@aws-sdk/client-ses": "3",
    "@aws-sdk/client-sns": "3",
    "serverless": "4",
    "serverless-offline": "14",
    "serverless-domain-manager": "8"
  }
}
