	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/zootella/cold3.git"
	},
	"scripts": {
		"local": "serverless offline --reloadHandler",
		"build": "node build.js && cd dist && serverless package && cd .. && node size.js && node ../test.js",
