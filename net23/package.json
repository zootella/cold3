{
	"name": "net23",
	"description": "Network 23",
	"license": "GPL-3.0-only",
	"version": "0.1.0",
	"type": "module",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/zootella/cold3.git"
	},
	"scripts": {
		"local": "serverless offline --reloadHandler",
		"build": "serverless package && yarn size",
		"size": "ls -la .serverless/net23.zip",
		"deploybefore": "yarn stowaway && yarn --cwd ../ seal set-cloud",
		"deployafter": "yarn --cwd ../ seal set-local && yarn --cwd ../ test && yarn size",
		"deploy": "yarn deploybefore && serverless deploy && yarn deployafter",
		"justdeploy": "serverless deploy --package .serverless",
		"stowaway": "node stowaway.js",
		"www": "aws s3 sync ./www s3://www-net23-cc --delete"
	},
	"dependencies": {
		"icarus": "*",
		"@supabase/supabase-js": "^2.39.8",
		"twilio": "^5.3.5",
		"@sendgrid/mail": "^8.1.4",
		"sharp": "^0.33.5"
	},
	"devDependencies": {
		"fs-extra": "^11.2.0",
		"@aws-sdk/client-ses": "^3.682.0",
		"@aws-sdk/client-sns": "^3.682.0",
		"serverless": "^4.4.11",
		"serverless-offline": "^14.3.4",
		"serverless-domain-manager": "^8.0.0"
	}
}
