//./wrangler.jsonc
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "oauth",
	"main": ".svelte-kit/cloudflare/_worker.js",
	"compatibility_date": "2025-06-10",
	//note that "compatibility_flags": ["nodejs_compat"] is intentionally omitted until something makes us turn that on
	"assets": {
		"binding": "ASSETS",
		"directory": ".svelte-kit/cloudflare"
	},
	"observability": {
		"enabled": true
	}
}
