{
	"name": "site4",
	"description": "Nuxt site on Cloudflare",
	"license": "GPL-3.0-only",
	"version": "0.1.0",
	"type": "module",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/zootella/cold3.git"
	},
	"scripts": {
		"local": "nuxt dev",
		"size": "nuxi analyze --no-serve && open ./size/client.html && open ./size/nitro.html",
		"build": "nuxt build",
		"deploy-before": "yarn --cwd ../ seal set-cloud",
		"deploy-after": "yarn --cwd ../ seal set-local && yarn --cwd ../ test",
		"deploy": "yarn deploy-before && yarn build && wrangler deploy && yarn deploy-after",
		"generate": "nuxt generate",
		"preview": "nuxt build && wrangler dev",
		"postinstall": "nuxt prepare",
		"retype": "wrangler types"
	},
	"dependencies": {
		"@pinia/nuxt": "^0.11.1",
		"@tanstack/vue-query": "^5.87.1",
		"@unhead/vue": "^2.0.5",
		"@wagmi/connectors": "^5.9.9",
		"@wagmi/core": "^2.20.3",
		"@wagmi/vue": "^0.2.5",
		"icarus": "*",
		"nuxt": "^3.17.5",
		"nuxt-og-image": "5.1.6",
		"pinia": "^3.0.3",
		"qrcode": "^1.5.4",
		"unstorage": "^1.15.0",
		"vidstack": "^1.12.13",
		"viem": "^2.37.3",
		"vue": "^3.5.16",
		"vue-router": "^4.5.1"
	},
	"devDependencies": {
		"@nuxtjs/tailwindcss": "^6.14.0",
		"@tailwindcss/forms": "^0.5.10",
		"nitro-cloudflare-dev": "^0.2.2",
		"nitropack": "^2.11.12",
		"nuxi": "^3.25.1",
		"pino-pretty": "^13.1.1",
		"wrangler": "^4.20.0"
	}
}
